{   
    "manifest_version": 3,
    "name": "Playback Speed",
    "description": "Control the playback speed of videos",
    "version": "1.0.1",
    "permissions":["storage","activeTab","scripting"],
    "action":{
        "default_icon":{
            "16": "images/icon.png",
            "24": "images/icon.png",
            "32": "images/icon.png",
            "48": "images/icon.png",
            "128": "images/icon.png"
        },
        "default_title": "Playback Speed",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    }
  }

